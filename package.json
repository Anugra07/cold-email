{
  "name": "cold-outreach-backend",
  "version": "1.0.0",
  "description": "Production-ready Express.js backend for automated cold-outreach platform",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:debug": "nodemon --inspect app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint . --ext .js,.json",
    "lint:fix": "eslint . --ext .js,.json --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "docker:build": "docker build -t cold-outreach-backend .",
    "docker:run": "docker run -p 3000:3000 cold-outreach-backend",
    "precommit": "npm run lint && npm run test",
    "docs": "swagger-jsdoc -d swagger.config.js routes/*.js -o swagger.json"
  },
  "keywords": [
    "express",
    "nodejs",
    "cold-outreach",
    "email-automation",
    "web-scraping",
    "ai-generation"
  ],
  "author": "Cold Outreach Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "celebrate": "^15.0.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "aws-sdk": "^2.1508.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "openai": "^4.20.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "email-validator": "^2.0.4",
    "cron": "^3.1.6",
    "agenda": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
